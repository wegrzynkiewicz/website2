<!doctype html>
<html lang="{{ language }}">
<head>
    <meta charset="UTF-8">
    {% block head_scripts %}
        {{ entries.map(injector.injectStyle) }}
        {{ entries.map(injector.injectScript) }}
    {% endblock %}
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <title>{{ title }}</title>
</head>
<body>
{% block document_content %}{% endblock %}
</body>
</html>
